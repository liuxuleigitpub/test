	<script type="text/javascript">
	$(function(){
		var curentTime = CurentTime();
		$('#starttime').val(curentTime);
		$('#endtime').val(curentTime);
		dataList(curentTime,curentTime);
	});
	function dataList(starttime,endtime){
		$.ajax({
			type: "POST",
			url: "../channel/admindataStatics",
			data: {starttime: starttime+' 00:00:00', endtime: endtime+' 23:59:59'},
			dataType: "json",
			success: function(data){
				$('#dataList').empty();
				if (data && data.list) {
					// 重新构建table
					$.each(data.list, function(i, row) {
					var detailurl = "detail_income_profile_1.jsp?cid="+row.channelId
							+"&starttime="+starttime+"&endtime="+endtime;
					if(starttime==endtime){
						detailurl = "detail_income_profile_2.jsp?cid="+row.channelId
						+"&starttime="+starttime+"&endtime="+endtime;
					}
					
					$('#dataList').append(
							'<tr>' +
							'<td align="left">' + row.channelId + '</td>' +
							'<td align="left">' + row.count + '</td>' +
							'<td align="left">' + row.fee + '</td>' +
							'<td align="left">' + row.klcs + '</td>' +
							'<td align="left">' + row.klje + '</td>' +
							'<td align="left">' + row.zccs + '</td>' +
							'<td align="left">' + row.zcje + '</td>' +
							'<td align="left">' + (row.klje/row.fee).toFixed(4) + '</td>' +
							'<td align="left"><a href="'+detailurl+'">查看详细</a></td>' +
							'</tr>');
					});
				}else{
					$('#dataList').append('<tr><td colspan="3">暂无数据</td></tr>');
				}
			}
		});
	}
	function queryData(){
		var starttime = $('#starttime').val();
		var endtime = $('#endtime').val();
		dataList(starttime,endtime)
		
	}
	function CurentTime()
    { 
        var now = new Date();
       
        var year = now.getFullYear();       //年
        var month = now.getMonth() + 1;     //月
        var day = now.getDate();            //日
       
        var clock = year + "-";
       
        if(month < 10)
            clock += "0";
       
        clock += month + "-";
       
        if(day < 10)
            clock += "0";
           
        clock += day;
       
        return(clock); 
    } 
	</script>
